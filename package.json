{
  "version": "1.0.0",
  "name": "@robertgregorywest/rob-west-gatsby",
  "description": "Portfolio website using Gatsby and Kontent headless CMS.",
  "repository": "git+https://github.com/robertgregorywest/rob-west-gatsby",
  "author": "Rob West <robertgregorywest@gmail.com> (https://github.com/robertgregorywest)",
  "license": "MIT",
  "private": false,
  "scripts": {
    "prepare": "node scripts/ncp.js .env.template .env -u",
    "develop": "gatsby develop",
    "build": "gatsby build",
    "clean": "gatsby clean",
    "lint": "./node_modules/.bin/eslint --cache --ext .js,.jsx --ignore-pattern public .",
    "test": "echo \"Error: no test specified\" && exit 1",
    "format": "prettier --write './*.js' 'src/**/*.js' 'src/**/*.jsx'",
    "deploy": "gatsby build --prefix-paths && gh-pages -d public"
  },
  "keywords": [
    "gatsby",
    "blog",
    "kontent",
    "CMS"
  ],
  "dependencies": {
    "@kontent-ai/gatsby-components": "^9.0.2",
    "@kontent-ai/gatsby-source": "^9.0.2",
    "gatsby-image": "^3.11.0",
    "gatsby-plugin-gtag": "^1.0.13",
    "gatsby-plugin-image": "^2.0.0",
    "gatsby-plugin-manifest": "^4.0.0",
    "gatsby-plugin-robots-txt": "^1.5.3",
    "gatsby-source-filesystem": "^4.0.0",
    "gatsby-transformer-sharp": "^4.0.0",
    "highlight.js": "^11.3.1",
    "highlightjs-cshtml-razor": "^2.0.0",
    "react": "^17.0.0",
    "react-dom": "^17.0.0",
    "react-helmet": "^6.1.0",
    "sass": "^1.45.0"
  },
  "devDependencies": {
    "babel-eslint": "^10.1.0",
    "bluebird": "^3.5.1",
    "dotenv": "^8.2.0",
    "eslint": "^7.9.0",
    "eslint-config-airbnb": "^18.1.0",
    "eslint-config-prettier": "^6.12.0",
    "eslint-loader": "^4.0.2",
    "eslint-plugin-import": "^2.21.1",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-prettier": "^3.1.3",
    "eslint-plugin-react": "^7.21.2",
    "gatsby": "^4.0.0",
    "gatsby-cli": "^5.0.0",
    "gatsby-plugin-catch-links": "^4.0.0",
    "gatsby-plugin-eslint": "^4.0.0",
    "gatsby-plugin-google-fonts": "latest",
    "gatsby-plugin-react-helmet": "^4.0.0",
    "gatsby-plugin-sass": "^5.0.0",
    "gatsby-plugin-sharp": "^4.0.0",
    "gatsby-plugin-sitemap": "^5.0.0",
    "lodash": "^4.17.4",
    "lost": "^8.2.0",
    "ncp": "^2.0.0",
    "postcss-pxtorem": "^5.1.1",
    "prettier": "^2.1.2"
  }
}
