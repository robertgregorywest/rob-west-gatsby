    "gatsby-plugin-robots-txt": "^2.0.0",
    "gatsby-source-filesystem": "^5.0.0",
    "gatsby-transformer-sharp": "^5.0.0",
    "highlight.js": "^11.3.1",
    "highlightjs-cshtml-razor": "^2.0.0",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "sass": "^1.45.0"
